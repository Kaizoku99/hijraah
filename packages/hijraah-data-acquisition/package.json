{
  "name": "@hijraah/data-acquisition",
  "version": "0.1.0",
  "description": "Data acquisition and competitive differentiation system for Hijraah",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./trigger": {
      "types": "./dist/trigger/index.d.ts",
      "import": "./dist/trigger/index.js",
      "require": "./dist/trigger/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "npx tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "trigger:dev": "npx trigger.dev@latest dev",
    "trigger:deploy": "npx trigger.dev@latest deploy"
  },
  "dependencies": {
    "@ai-sdk/mistral": "catalog:",
    "@ai-sdk/openai": "catalog:",
    "@ai-sdk/react": "^1.2.12",
    "@opentelemetry/exporter-logs-otlp-http": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "@supabase/supabase-js": "catalog:",
    "@trigger.dev/sdk": "catalog:",
    "ai": "catalog:",
    "date-fns": "^3.0.0",
    "drizzle-kit": "catalog:",
    "drizzle-orm": "catalog:",
    "firecrawl": "catalog:",
    "nanoid": "^5.0.0",
    "p-queue": "^8.0.0",
    "p-retry": "^6.0.0",
    "postgres": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "immigration",
    "data-acquisition",
    "trigger.dev",
    "supabase",
    "firecrawl"
  ],
  "author": "Hijraah Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hijraah/hijraah.git",
    "directory": "packages/hijraah-data-acquisition"
  }
}