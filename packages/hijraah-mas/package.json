{
  "name": "@hijraah/mas",
  "version": "0.1.0",
  "description": "Multi-Agent System using AI SDK v5 for immigration processing workflows",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./orchestrators": {
      "types": "./dist/orchestrators/index.d.ts",
      "import": "./dist/orchestrators/index.js"
    },
    "./teams": {
      "types": "./dist/teams/index.d.ts",
      "import": "./dist/teams/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "npx tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "ai": "catalog:",
    "@ai-sdk/openai": "catalog:",
    "@ai-sdk/anthropic": "catalog:",
    "@ai-sdk/mistral": "catalog:",
    "@ai-sdk/google": "catalog:",
    "@ai-sdk/deepseek": "catalog:",
    "@ai-sdk/fireworks": "catalog:",
    "@supabase/supabase-js": "catalog:",
    "zod": "catalog:",
    "drizzle-orm": "catalog:",
    "@trigger.dev/sdk": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "tsup": "catalog:",
    "vitest": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:"
  },
  "peerDependencies": {
    "@hijraah/types": "workspace:*",
    "@hijraah/utils": "workspace:*"
  }
}