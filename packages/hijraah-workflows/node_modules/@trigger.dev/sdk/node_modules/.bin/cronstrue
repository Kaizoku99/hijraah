#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/downloads/Hijraah/node_modules/.pnpm/cronstrue@2.61.0/node_modules/cronstrue/bin/node_modules:/mnt/e/downloads/Hijraah/node_modules/.pnpm/cronstrue@2.61.0/node_modules/cronstrue/node_modules:/mnt/e/downloads/Hijraah/node_modules/.pnpm/cronstrue@2.61.0/node_modules:/mnt/e/downloads/Hijraah/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/downloads/Hijraah/node_modules/.pnpm/cronstrue@2.61.0/node_modules/cronstrue/bin/node_modules:/mnt/e/downloads/Hijraah/node_modules/.pnpm/cronstrue@2.61.0/node_modules/cronstrue/node_modules:/mnt/e/downloads/Hijraah/node_modules/.pnpm/cronstrue@2.61.0/node_modules:/mnt/e/downloads/Hijraah/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../cronstrue@2.61.0/node_modules/cronstrue/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../cronstrue@2.61.0/node_modules/cronstrue/bin/cli.js" "$@"
fi
