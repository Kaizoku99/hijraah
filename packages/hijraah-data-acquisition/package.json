{
  "name": "@hijraah/data-acquisition",
  "version": "0.1.0",
  "description": "Data acquisition and competitive differentiation system for Hijraah",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./trigger": {
      "types": "./dist/trigger/index.d.ts",
      "import": "./dist/trigger/index.js",
      "require": "./dist/trigger/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "trigger:dev": "npx trigger.dev@latest dev",
    "trigger:deploy": "npx trigger.dev@latest deploy"
  },
  "dependencies": {
    "@trigger.dev/sdk": "^4.0.0-beta",
    "@supabase/supabase-js": "^2.39.0",
    "drizzle-orm": "^0.29.0",
    "drizzle-kit": "^0.20.0",
    "ai": "^3.0.0",
    "firecrawl-mcp": "^1.0.0",
    "zod": "^3.22.0",
    "date-fns": "^3.0.0",
    "p-queue": "^8.0.0",
    "p-retry": "^6.0.0",
    "nanoid": "^5.0.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "keywords": [
    "immigration",
    "data-acquisition",
    "trigger.dev",
    "supabase",
    "firecrawl"
  ],
  "author": "Hijraah Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hijraah/hijraah.git",
    "directory": "packages/hijraah-data-acquisition"
  }
}
